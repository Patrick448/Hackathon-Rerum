group java;


prog(classList) ::= <<

package generatedodl;

<classList>
>>

classList(classes) ::= <<
<classes; separator="\n\n">
>>

class(name, declist) ::= <<
public class <name>{
    <declist>

    public void print(){
        System.out.println("Hello World");
    }
}
>>

declist(decls) ::= <<
<decls; separator="\n">
>>

decl(type, name) ::= <<
public <type> <name>;
>>

raw_text(text) ::= <<
<text>
>>


